<!doctype html>
<html>
  <head>
    <meta charset='utf-8' />
    <%= wicked_pdf_stylesheet_link_tag "pdf" -%>
    <%= wicked_pdf_javascript_include_tag "number_pages" %>
  </head>
  <body style="font-family: 'Overpass', sans-serif;" onload='number_pages'>
    <div id="header" style="text-align: center;padding-top: 1.7em;">
      <%= wicked_pdf_image_tag 'logo-qr-meta.png', width: '300' %>
    </div>
    <div id="content">
      <%= yield %>
    </div>
  </body>
</html>
